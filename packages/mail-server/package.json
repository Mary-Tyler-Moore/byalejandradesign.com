{
  "name": "@artetexture/mail-server",
  "version": "0.0.1",
  "private": true,
  "description": "for sending mails",
  "license": "ISC",
  "author": "Nick Myers <nickmyers111@gmail.com>",
  "main": "build/app.js",
  "scripts": {
    "clean": "shx rm -rf build",
    "stage": "run-s rollup:build serverless:staging",
    "deploy": "run-s rollup:build serverless:production",
    "develop": "run-p rollup:watch serverless:development",
    "prerollup:build": "run-s clean",
    "rollup:build": "rollup -c rollup.config.js",
    "prerollup:watch": "run-s clean",
    "rollup:watch": "rollup -c rollup.config.js --watch",
    "serverless:development": "serverless offline start --stage development --port 3001",
    "serverless:production": "serverless deploy -v --stage production",
    "serverless:staging": "serverless deploy -v --stage staging"
  },
  "dependencies": {
    "@artetexture/checkout-objects": "^0.0.1",
    "@artetexture/server-middleware": "^0.0.1",
    "@artetexture/server-env": "^0.0.1",
    "@artetexture/templates": "^0.0.1",
    "@babel/runtime": "^7.1.2",
    "@njmyers/sass": "^0.0.15",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "express": "^4.16.2",
    "mailgun-js": "^0.21.0",
    "react": "16.4",
    "react-dom": "16.4",
    "serverless-http": "^1.6.0",
    "serverless-plugin-monorepo": "^0.4.0",
    "smalldash": "^1.1.20"
  },
  "devDependencies": {
    "flow-bin": "^0.83.0",
    "flow-mono-cli": "^1.4.2",
    "flow-typed": "^2.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "serverless": "^1.28.0",
    "serverless-domain-manager": "^2.3.0",
    "serverless-offline": "^3.16.0",
    "shx": "^0.3.2"
  }
}
