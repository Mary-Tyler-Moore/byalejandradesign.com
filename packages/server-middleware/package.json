  "devDependencies": {
    "flow-bin": "^0.83.0",
    "flow-copy-source": "^2.0.2",
    "flow-mono-cli": "^1.4.2",
    "flow-typed": "^2.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "shx": "^0.3.2"
  },
  "peerDependencies": {
    "express": "^4.16.2"
  }
