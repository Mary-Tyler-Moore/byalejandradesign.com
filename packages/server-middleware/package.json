{
  "name": "@artetexture/server-middleware",
  "version": "0.0.1",
  "description": "shared server middleware package for express",
  "files": [
    "build"
  ],
  "main": "build/index.cjs.js",
  "module": "build/index.js",
  "scripts": {
    "build:cjs": "rollup -c rollup.config.js",
    "build:esm": "babel src --out-dir build",
    "build:flow": "flow gen-flow-files src --out-dir build",
    "prebuild": "shx rm -rf build",
    "build": "run-s build:cjs build:esm build:flow",
    "develop": "rollup -c rollup.config.js --watch"
  },
  "author": "Nick Myers <nickmyers111@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "smalldash": "^1.1.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "flow-bin": "^0.83.0",
    "flow-copy-source": "^2.0.2",
    "flow-typed": "^2.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "shx": "^0.3.2"
  },
  "peerDependencies": {
    "express": "^4.16.2"
  }
}
