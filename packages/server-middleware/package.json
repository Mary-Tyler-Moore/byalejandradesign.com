{
  "name": "@byalejandradesign/server-middleware",
  "version": "0.0.1",
  "private": true,
  "description": "shared server middleware package for express",
  "repository": {
    "type": "git",
    "url": "https://github.com/njmyers/byalejandradesign.com.git"
