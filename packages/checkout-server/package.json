{
  "name": "@artetexture/checkout-server",
  "version": "0.0.1",
  "description": "a braintree server",
  "private": true,
  "keywords": [
    "braintree"
  ],
  "license": "ISC",
  "author": "Nick Myers <nickmyers111@gmail.com>",
  "main": "build/app.cjs.js",
  "scripts": {
    "develop": "run-p rollup:watch serverless:development",
    "stage": "run-p rollup:build serverless:staging",
    "deploy": "run-p rollup:build serverless:production",
    "rollup:build": "shx rm -rf build && rollup -c rollup.config.js",
    "rollup:watch": "shx rm -rf build && rollup -c rollup.config.js --watch",
    "serverless:development": "serverless offline start --stage development",
    "serverless:staging": "serverless deploy -v --stage staging",
    "serverless:production": "serverless deploy -v --stage production"
  },
  "dependencies": {
    "@artetexture/checkout-objects": "^0.0.1",
    "@artetexture/server-middleware": "^0.0.1",
    "@babel/runtime": "^7.1.2",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "braintree": "^2.11.0",
    "express": "^4.16.2",
    "serverless-http": "^1.6.0",
    "smalldash": "^1.1.20",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "flow-bin": "^0.83.0",
    "flow-mono-cli": "^1.4.2",
    "flow-typed": "^2.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "serverless": "^1.28.0",
    "serverless-domain-manager": "^2.3.0",
    "serverless-offline": "^3.16.0",
    "shx": "^0.3.2"
  }
}
